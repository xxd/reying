<h1>Listing order_details</h1>

<table>
  <tr>
    <th>Order</th>
    <th>Schedule</th>
    <th>Adjustedprice</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @order_details.each do |order_detail| %>
  <tr>
    <td><%= order_detail.order %></td>
    <td><%= order_detail.schedule %></td>
    <td><%= order_detail.adjustedprice %></td>
    <td><%= link_to 'Show', order_detail %></td>
    <td><%= link_to 'Edit', edit_order_detail_path(order_detail) %></td>
    <td><%= link_to 'Destroy', order_detail, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

